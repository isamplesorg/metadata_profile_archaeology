@prefix test2: <https://w3id.org/isample/vocabulary/test/2/> .
@prefix test3: <https://w3id.org/isample/vocabulary/test/3/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

test2:testvocabulary
    rdf:type skos:ConceptScheme ;
    skos:definition "Provides a test vocabulary with 33 statements"@en ;
    skos:prefLabel "Vocab 2 test vocabulary"@en ;
.

test2:Vocab2rootterm
    rdf:type skos:Concept ;
    skos:definition "The root concept of the test vocabulary"@en ;
    skos:prefLabel "Vocab 2 root"@en ;
    skos:inScheme test2:testvocabulary ;
    skos:topConceptOf test2:testvocabulary ;
.

test2:level1a
    rdf:type skos:Concept ;
    skos:inScheme test2:testvocabulary ;
    skos:definition "A term once removed from root"@en ;
    skos:prefLabel "Vocab 2 level1a"@en ;
    skos:broader test2:rootterm ;
.

test2:level1b
    rdf:type skos:Concept ;
    skos:inScheme test2:testvocabulary ;
    skos:definition "A term once removed from root"@en ;
    skos:prefLabel "Vocab 2 level1b"@en ;
    skos:broader test2:rootterm ;
.

test2:level2aa
    rdf:type skos:Concept ;
    skos:inScheme test2:testvocabulary ;
    skos:definition "A term twice removed from root"@en ;
    skos:prefLabel "Vocab 2 level2aa"@en ;
    skos:broader test2:level1a ;
.

test2:level2ab
    rdf:type skos:Concept ;
    skos:inScheme test2:testvocabulary ;
    skos:definition "A term twice removed from root"@en ;
    skos:prefLabel "Vocab 2 level2ab"@en ;
    skos:broader test2:level1a ;
.

test2:level11
    rdf:type skos:Concept ;
    skos:inScheme test2:testvocabulary ;
    skos:definition "A term in another vocabulary referencing test2"@en ;
    skos:prefLabel "Vocab 2 level11"@en ;
    skos:broader test3:rootterm ;
.


